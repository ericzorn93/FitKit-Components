{
  "name": "fitkit-components",
  "version": "0.0.1",
  "description": "A component library with React and TypeScript for the fitness and lifestyle application, FitKit.",
  "author": "Eric Zorn <zornwebdev@gmail.com>",
  "license": "MIT",
  "repository": "ericzorn93/FitKit-Components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "node-sass": "^4.13.1"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@svgr/rollup": "^4.3.3",
    "@types/eslint": "^6.1.2",
    "@types/jest": "^24.0.18",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "gh-pages": "^2.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-url": "^2.2.2",
    "typescript": "^3.6.3"
  },
  "files": [
    "dist"
  ]
}
